.container
    margin auto
    max-width 700px
    text-align center

.grid
    margin 34px auto 0
    max-width 420px

.grid-row
    clear both
    float left

.grid-tile
    cursor pointer
    float left
    height 91px
    line-height 57px
    margin-top -34px
    position relative
    width 70px

    &:hover
        top -7px

.water
    background url('../images/water.png') no-repeat
    color #fff
    font-family sans-serif

.visited
    opacity 0.25

.cursor
    opacity 0.75